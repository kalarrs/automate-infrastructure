{
  "extends": "nx/presets/core.json",
  "npmScope": "kalarrs-automate-infrastructure",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "defaultProject": "api-echo",
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "libs"
  }
}
