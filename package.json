{
  "name": "kalarrs-automate-infrastructure",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "g:aws:tg": "yarn run generate:infrastructure:aws:terragrunt --useInitCwd",
    "app:api:echo:build:dev": "nx build api-echo --configuration=development",
    "clean:terraform:cache": "find . -type d -name '.terraform' | xargs rm -rf",
    "clean:terragrunt:cache": "find . -type d -name \".terragrunt-cache\" | xargs rm -rf",
    "generate:infrastructure:provider": "nx workspace-generator infrastructure-provider",
    "generate:infrastructure:aws:terragrunt": "nx workspace-generator infrastructure-aws-terragrunt",
    "generate:aws:labs:dev:us-west-2": "nx workspace-generator infrastructure-aws-terragrunt --profile=labs --stage=dev --env='' --region=us-west-2",
    "generate:project:node": "nx g @nrwl/node:app",
    "generate:project:lambda": "nx workspace-generator lambda-app",
    "generate:lambda:function": "nx workspace-generator lambda-function",
    "generate:lib": "nx g @nrwl/workspace:lib",
    "generate:lib:js": "nx g @nrwl/workspace:lib --js",
    "generate:workspace:generator": "nx generate @nrwl/workspace:workspace-generator",
    "patch": "cp ./.tmp/webpack.impl.js ./node_modules/@nrwl/node/src/executors/webpack"
  },
  "private": true,
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.58.0",
    "@kalarrs-automate-infrastructure/shared-webpack-lambda": "link:./libs/shared/webpack/lambda/src",
    "aws-xray-sdk-core": "^3.3.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@aws-sdk/shared-ini-file-loader": "^3.58.0",
    "@nrwl/cli": "14.0.5",
    "@nrwl/devkit": "14.0.5",
    "@nrwl/eslint-plugin-nx": "14.0.5",
    "@nrwl/jest": "14.0.5",
    "@nrwl/js": "14.0.5",
    "@nrwl/linter": "14.0.5",
    "@nrwl/node": "14.0.5",
    "@nrwl/workspace": "14.0.5",
    "@phenomnomnominal/tsquery": "^4.2.0",
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.5.1",
    "nx": "14.0.5",
    "prettier": "^2.5.1",
    "prompts": "^2.4.2",
    "ts-jest": "27.1.4",
    "typescript": "~4.5.2",
    "webpack-merge": "^5.8.0",
    "yargs": "^17.4.1"
  }
}

